CREATE TABLE CachedDataSources (
    identifier TEXT NOT NULL UNIQUE,
    sourceType TEXT NOT NULL,
    content TEXT NOT NULL
);

selectAll:
SELECT * FROM CachedDataSources;

selectById:
SELECT *
FROM CachedDataSources
WHERE identifier = ?;

insert:
INSERT INTO CachedDataSources(identifier, sourceType, content)
VALUES (?,?,?);

update:
UPDATE CachedDataSources
SET content = ?
WHERE identifier = ?;

delete:
DELETE FROM CachedDataSources
WHERE identifier = ?;